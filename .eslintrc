{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": 0,
    "no-tabs": 0,
    "linebreak-style": 0,
    "react/jsx-indent": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-quotes": 0,
    "class-methods-use-this": 0,
    "react/jsx-indent-props": 0,
    "jsx-a11y/label-has-for": 0,
    "react/forbid-prop-types": 0
  },
  "globals": {
    "describe": true,
    "it": true
  }
}